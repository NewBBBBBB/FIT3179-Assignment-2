          }
      },
      {
          "name": "select",
          "select": {
              "type": "point",
              "fields": ["state"]
          }
      }
  ],
  "transform": [
      {
          "lookup": "state",
          "from": {
              "data": {
                  "url": "https://raw.githubusercontent.com/NewBBBBBB/FIT3179-Assignment-2/refs/heads/main/data/malaysia.json",
                  "format": {
                      "type": "topojson",
                      "feature": "geoBoundaries-MYS-ADM1_simplified"
                  }
              },
              "key": "properties.shapeName"
          },
          "as": "geo"
      }
  ],
  "mark": {
      "type": "geoshape"
  },
  "encoding": {
      "shape": {
          "field": "geo",
          "type": "geojson"
      },
      "color": {
          "field": "u_rate",
          "type": "quantitative",
          "scale": {
              "scheme": "greens",
              "type": "quantize",
              "domain": [1, 7.5]
          },
          "legend": {
              "format": ".2s",
              "title": "Unemployment Rate (%)"
          }
      },
      "strokeWidth": {
          "value": 1
      },
      "stroke": {
          "value": "black"
      },
      "tooltip": [
          {
              "field": "state",
              "type": "nominal",
              "title": "State"
          },
          {
              "field": "u_rate",
              "type": "quantitative",
              "title": "Unemployment Rate",
              "format": ","
          }
      ]
  }
